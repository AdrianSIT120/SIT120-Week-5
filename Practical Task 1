-------------------------HTML-------------------------
<div id="app-7">
  <ol>
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>

    <todo-item
      v-for="item in tasklist"
      v-bind:todo="item"
      v-bind:key="item.id"
    ></todo-item>
    
  </ol>
</div>
-------------------------HTML-------------------------




-------------------------CSS-------------------------
html, body {
    margin: 5px;
    padding: 0;
}
-------------------------CSS-------------------------




-------------------------Javascript-------------------------
Vue.component('todo-item', {
  props: ['todo'],
  template: '<li>{{ todo.text }}</li>'
})

var app7 = new Vue({
  el: '#app-7',
  data: {
    tasklist: [
      { id: 0, text: 'Assignment for english unit' },
      { id: 1, text: 'Assignment for emath unit' },
      { id: 2, text: 'Assignment for science unit' },
      { id: 3, text: 'Assignment for computing unit' }
    ]
  }
})
